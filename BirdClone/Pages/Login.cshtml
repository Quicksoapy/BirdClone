@page "/Login"
@model BirdClone.Pages.Login

@{
    Layout = null;
}

@if (Request.Cookies["UserId"] != "0")
{
    Redirect("/Index");
}

<form method="post">
    <label>Username: </label><input type="text" asp-for="LoginModel.Username"/>
    <label>Password: </label><input type="text" asp-for="LoginModel.Password"/>
    <button type="submit">Submit</button>
</form>